<h1>Change Team Info</h1>

<%= form_with model:@team do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name %><br>
    <h4>Landscape</h4>
    <%= form.fields_for :landscapes do |land| %>
        <%= land.label :address %>
        <%= land.text_field :address %>
        <%= land.label "Area (square feet)" %>
        <%= land.number_field :area %>
        <%= land.label :user_id %>
        <%= land.number_field :user_id %> <br>
    <% end %>
    <%= form.submit "Submit" %>

<%end %>